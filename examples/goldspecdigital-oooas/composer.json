{
    "autoload": {
        "psr-4": {
            "Sourceability\\Portal\\Demo\\": "."
        }
    },
    "require": {
        "goldspecdigital/oooas": "^2.10",
        "sourceability/portal": "@dev"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../",
            "options": {
                "symlink": true
            }
        }
    ],
    "require-dev": {
        "symfony/var-dumper": "^6.0"
    }
}
